@Library('karthik-jenkins-library') _

pipeline{
    agent any

    stages {

        stage('git checkout:git'){
            steps{

            gitcheckout(
                branch: "master",
                url: "https://github.com/lekkalakarthik/javacode.git"
            )
                
            }
        }
        stage('Unit test: Maven'){
            steps{
                script{
                    mvntest()
                }
            }
        }
        }
        
    }
